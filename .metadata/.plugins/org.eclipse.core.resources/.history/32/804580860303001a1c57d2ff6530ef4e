from selenium import webdriver
from time import sleep


driver = webdriver.Ie("E:\\Softwares\\My PC Apps\\Selenium Python\\Webdrivers\\IEDriverServer_Win32_3.9.0\\IEDriverServer.exe")

driver.get("http://demosite.center/wordpress/wp-login.php")

driver.set_page_load_timeout(30)

driver.maximize_window()

driver.implicitly_wait(20)

driver.get_screenshot_as_file("E:\\Softwares\\My PC Apps\\Selenium Python\\workspace\\SeleniumPythonDemo\\Screenshots\\WordpressLoginPage.png")

print(driver.title)

assert "Wordpress" in driver.title

driver.find_element_by_id("user_login").send_keys("admin")

driver.find_element_by_name("pwd").send_keys("demo123")

driver.find_element_by_name("wp-submit").click()

sleep(2)

driver.get_screenshot_as_file("E:\\Softwares\\My PC Apps\\Selenium Python\\workspace\\SeleniumPythonDemo\\Screenshots\\WordpressHomePage.png")

driver.quit()